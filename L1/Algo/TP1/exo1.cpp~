#include <iostream>
using namespace std;

int main()
{
	int note=0;
	do
	{
		cout<<"Entrez la note";
		cin>>note;
	}
	while(note>20 || note < 0);
	if (note<10)
		cout<<"Insuffisant";
	else if (note<12)
		cout<<"Passable";
	else if (note<14)
		cout<<"Assez Bien";
	else if (note<16)
		cout<<"Bien";
	else
		cout<<"TrÃ¨s bien";
	return 0;
}	
